# Generated by Chef for <%= node['fqdn'] %>
# Local modifications will be overridden
<% Array(@log_paths).each do |log_path| -%>

<%= log_path %> {
  <%= @frequency %>
  <% if @triggered_log_size %>
  size <%= @triggered_log_size %>
  <% end -%>
  rotate <%= @rotation_limit %>
  <% if @creating_log %>
  create <%= @creating_log %>
  <% else -%>
  nocreate
  <% end -%>
  <% if @moving_log %>
  olddir <%= @moving_log %>
  <% else -%>
  noolddir
  <% end -%>
  <% if @allow_missing_log -%>
  missingok
  <% end -%>
  <% if @compress -%>
  compress
  <% end %>
  <% if @delay_compress -%>
  delaycompress
  <% end %>
  <% if @date_ext -%>
  dateext
  <% end %>
  <% if @copy -%>
  copy
  <% if @truncate_copy -%>
  copytruncate
  <% end -%>
  <% end -%>
  <% if @deny_empty_rotation -%>
  notifempty
  <% end -%>
  <% if @first_action -%>
  firstaction
    <%= @first_action %>
  endscript
  <% end -%>
  <% if @prerotate -%>
  prerotate
    <%= @prerotate %>
  endscript
  <% end -%>
  <% if @postrotate -%>
  postrotate
    <%= @postrotate %>
  endscript
  <% end -%>
  <% if @last_action -%>
  lastaction
    <%= @last_action %>
  endscript
  <% end -%>
}
<% end -%>
